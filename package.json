{
    "name": "voice-commands",
    "displayName": "Voice Commands",
    "description": "Listen user's voice commands",
    "version": "0.0.1",
    "publisher": "lanly-dev",
    "engines": {
        "vscode": "^1.8.1"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:greeting"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "greeting",
            "title": "Voice Commands"
        }]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}